{
  "name": "lunchabler",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "mkdir dist/javascripts && touch dist/javascripts/app.js && babel ./public/javascripts/app.js -o dist/javascripts/app.js && babel ./routes -d dist/javascripts/routes",
    "dev": "nodemon ./bin/www --exec babel-node",
    "start": "node ./bin/www"
  },
  "dependencies": {
    "babel": "^6.23.0",
    "body-parser": "~1.16.0app",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.0",
    "express": "~4.14.1",
    "jade": "~1.11.0",
    "mongodb": "^2.2.25",
    "monk": "^4.0.0",
    "morgan": "~1.7.0",
    "serve-favicon": "~2.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "nodemon": "^1.11.0"
  }
}
