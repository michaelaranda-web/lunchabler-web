{
  "name": "lunchabler",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm run clean && npm run build-server && webpack",
    "build-server": "babel -d ./build ./server -s",
    "clean": "rm -rf build && mkdir build && rm -rf dist && mkdir dist",
    "dev": "nodemon ./bin/www --exec babel-node",
    "heroku-prebuild": "npm run build",
    "postinstall": "npm install",
    "start": "node ./bin/www"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "body-parser": "~1.16.0app",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.0",
    "express": "~4.14.1",
    "jade": "~1.11.0",
    "mongodb": "^2.2.25",
    "monk": "^4.0.0",
    "morgan": "~1.7.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "serve-favicon": "~2.3.2",
    "webpack": "^2.5.1"
  },
  "devDependencies": {
    "nodemon": "^1.11.0",
    "webpack-dev-server": "^2.4.5"
  }
}
